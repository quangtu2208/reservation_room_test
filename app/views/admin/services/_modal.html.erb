<div class="modal fade" id="deleteModal" tabindex="-1" role="dialog" aria-labelledby="deleteModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-sm" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="deleteModalLabel"><%= t ".confirm" %></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-footer">
        <%= link_to t(".del"), admin_service_path(@service), method: :delete, class: "btn btn-danger" %>
        <button type="button" class="btn btn-light" data-dismiss="modal"><%= t ".cancel" %></button>
      </div>
    </div>
  </div>
</div>
