<%= provide :title, t(".page_title") %>

<div class="row">
  <div class="col-md-12 card">
    <div class="card-body">
      <h4 class="card-title"><%= t ".page_title" %></h4>
      <%= render "shared/search", path: search_admin_bed_details_path, fields: Settings.search_fields.bed_detail %>
      <table class="table">
        <thead>
          <tr>
            <th><%= t ".name" %></th>
            <th><%= t ".create_at" %></th>
            <th><%= t ".update_at" %></th>
          </tr>
        </thead>
        <tbody>
          <%= render @bed_details %>
        </tbody>
      </table>
      <%= paginate @bed_details %>
    </div>
  </div>
</div>
